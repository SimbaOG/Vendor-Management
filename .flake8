[flake8]
ignore = C101,D403,F403,F405,I100,I201,W503,W504,Q000
exclude =
        .git,
        __pycache__,
        .idea,
        Vendor-Management.iml,
        .mypy_cache

filename = *.py
accept-encodings = utf-8
inline-quotes = single
max-complexity = 12
max-linenumber = 500
max-line-length = 119
multiline-quotes = double
per-file-ignores =
    core/project/settings/templates/settings.dev.py:F821